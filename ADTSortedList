package cpp;

@SuppressWarnings("rawtypes")
public class ADTSortedList extends LinkList implements SortedList{

	private int[] arr;

	@Override
	public void add(Object newEntry) {
		if (arr.isEmpty() = false){
	        int n = arr.length;
	        for (int i = 0; i < n-1; i++) {
	            int min_idx = i;
	            for (int j = i+1; j < n; j++)
	                if ([j] < arr[min_idx])
	                    min_idx = j;
	            int temp = arr[min_idx];
	            arr[min_idx] = arr[i];
	            arr[i] = temp;
	        }
		}
	}
	@SuppressWarnings("null")
	@Override
	public Node remove() {
		int indexToRemove=0;
		arr[indexToRemove] = (Integer) null;
		return null;

	}

	@Override
	public Object peek() {
		return index[0];
	}

	@SuppressWarnings("null")
	@Override
	public void clear() {
		for(int i=0; i<arr.length; ++i) {
			arr[i] = (Integer) null;
		}

	}

}
